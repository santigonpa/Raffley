<div class="estimator">
  <h1>Raffle Estimator</h1>

  <section>
  <button phx-click="add" phx-value-quantity="5">
    +
  </button>
    <div>
      <%= @tickets %>
    </div>
    @
    <div>
      $<%= @price %>
    </div>
    =
    <div>
      $<%= @tickets * @price %>
    </div>
  </section>
</div>
